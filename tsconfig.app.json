/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@services/*": [
        "app/services/*"
      ],
      "@components/*": [
        "app/components/*"
      ],
      "@common/*": [
        "app/common/*"
      ],
      "@config/*": [
        "app/config/*"
      ]
    },
    "outDir": "./out-tsc/app",
    "types": []
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
